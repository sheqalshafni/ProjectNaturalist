<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.caffeinne.naturalisttracker">

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_logo"
        android:label="@string/home_toolbar_title"
        android:roundIcon="@drawable/ic_logo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".HabitatsActivity.WetlandHabitatsActivity"></activity>
        <activity android:name=".HabitatsActivity.DesertHabitatsActivity" />
        <activity
            android:name=".HabitatsActivity.FarmlandHabitatActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".HomeActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>